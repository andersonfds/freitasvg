{
  "name": "freitasvg",
  "version": "1.0.2",
  "description": "Converts SVG to Glyphs",
  "main": "dist/cjs/index.js",
  "module": "dist/es/index.js",
  "types": "dist/index.d.ts",
  "repository": "github:andersonfds/freitasvg",
  "keywords": [
    "svg",
    "font",
    "glyph"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "ttsc -p tsconfig.build.json && ttsc -p tsconfig.build.json --module commonjs --outDir dist/cjs",
    "prestart": "rimraf dist",
    "start": "nodemon --quiet main.ts"
  },
  "author": "Anderson Freitas",
  "license": "ISC",
  "devDependencies": {
    "@types/opentype.js": "^1.3.8",
    "canvas": "^2.11.2",
    "nodemon": "^3.1.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.2.1",
    "ttypescript": "^1.5.12",
    "typescript": "^4.4.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "dependencies": {
    "linkedom": "^0.18.5",
    "opentype.js": "^1.3.4"
  },
  "files": [
    "dist"
  ]
}
